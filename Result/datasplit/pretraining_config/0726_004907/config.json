{
    "name": "pretraining_config",
    "n_gpu": 1,
    "dataset": {
        "type": "MLPDataset",
        "args": {
            "seq_dir": "D:/tokenized_300000.npy",
            "vocab_dir": "/snp_data",
            "seed": 0,
            "max_len": 32,
            "test_split": 0.001
        }
    },
    "arch": {
        "type": "MlpRegression",
        "args": {}
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 5e-05,
            "weight_decay": 0
        }
    },
    "loss": "MSE_loss",
    "metrics": [
        "rmse",
        "mae"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 10,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 5,
        "save_dir": "../Result/",
        "save_period": 200,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 3,
        "tensorboard": false
    },
    "local_rank": null
}