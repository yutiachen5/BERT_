{
    "name": "finetuning_config",
    "n_gpu": 1,

    "data_loader": {
        "type": "EmbeddedDataset",
        "args":{
            "seed": 0,
            "batch_size": 512,
            "validation_split": 0.1,
            "test_split": 0.0,
            "num_workers": 1,
            "smiles_dir": "../ProcessedData/GDSC_SMILES_merged.csv",
            "pretrained_mdl_dir": "../Code/dataloader/embedding/snp_embedding/0811_113404",
            "downstream_data_dir": "../ProcessedData/downstream_data/",
            "test_size": 100,
            "shuffle": true
        }
    },

    "arch": {
        "type": "MlpRegression",
        "args": {

        }
    },

    "optimizer": {
        "type": "AdamW",
        "args":{
            "lr": 5e-5,
            "weight_decay": 0
        }
    },
    "loss": "MSE_loss",
    "metrics": [
        "rmse", "mae"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 10,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 5,
        "save_dir": "../Result/",
        "save_period": 200,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 3,
        "tensorboard": false
    }
}